{% extends "main.twig" %}
{% block content %}
    <div class="form-container">
        <form id="uploadImages" action="/upload" method="POST" enctype="multipart/form-data">
            <input type="submit" value=" запостить " class="submit"/>

            <div>
                <label for="title">Название женщины</label>
                <input type="text" name="title" id="title"/>
            </div>
            <div>
                <label for="file">Фотка</label>
                <input type="file" name="image" id="file"/>
            </div>
            <div class="clear"></div>
        </form>
    </div>
    <a href="#">ещё</a>
{% endblock %}